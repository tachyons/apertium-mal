install-apertium_en_hiDATA: $(apertium_en_hi_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(apertium_en_hidir)" || $(mkdir_p) "$(DESTDIR)$(apertium_en_hidir)"
	@list='$(apertium_en_hi_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(apertium_en_hiDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(apertium_en_hidir)/$$f'"; \
	  $(apertium_en_hiDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(apertium_en_hidir)/$$f"; \
	done

uninstall-apertium_en_hiDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(apertium_en_hi_DATA)'; for p in $$list; do \
	  f=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(apertium_en_hidir)/$$f'"; \
	  rm -f "$(DESTDIR)$(apertium_en_hidir)/$$f"; \
