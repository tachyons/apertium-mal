<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="mal-twol">
    <pipeline>
      <program name="hfst-strings2fst -S"/>
      <program name="hfst-compose-intersect">
        <file name=".deps/mal.twol.hfst"/>
      </program>
      <program name="hfst-fst2strings"/>
    </pipeline>
  </mode>

  <mode name="mal-morph">
    <pipeline>
      <program name="lt-proc -w">
        <file name="mal.automorf.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="mal-lexc">
    <pipeline>
      <program name="hfst-proc -g">
        <file name=".deps/mal.lexc.hfst"/>
      </program>
    </pipeline>
  </mode>


  <mode name="mal-gener">
    <pipeline>
      <program name="lt-proc -g">
        <file name="mal.autogen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="mal-tagger">
    <pipeline>
      <program name="lt-proc -w">
        <file name="mal.automorf.bin"/>
      </program>
      <program name="cg-proc -w -n -1">
        <file name="mal.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="mal-disam">
    <pipeline>
      <program name="lt-proc -w">
        <file name="mal.automorf.bin"/>
      </program>
      <program name="cg-conv -a -l" />
      <program name="vislcg3 --trace -g">
        <file name="apertium-mal.mal.rlx"/>
      </program>
    </pipeline>
  </mode>


</modes>
